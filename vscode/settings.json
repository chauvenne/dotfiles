{
    "editor.fontFamily": "JetBrains Mono Regular",
    "editor.fontLigatures": true,
    "editor.lineNumbers": "on",
    "editor.suggestSelection": "first",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "python.jediEnabled": false,
    "python.linting.pylintEnabled": true,
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\wsl.exe",
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.fontFamily": "DejaVu Sans Mono for Powerline",
    "terminal.integrated.shell.linux": "/bin/zsh",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": ["#434C5E"],
    "vim.statusBarColors.insert": "#C23B22",
    "vim.statusBarColors.visual": "#957DAD",
    "vim.statusBarColors.visualline": "#957DAD",
    "vim.statusBarColors.visualblock": "#A3BE8C",
    "vim.statusBarColors.replace": "#D08770",
    "vim.statusBarColors.commandlineinprogress": "#007ACC",
    "vim.statusBarColors.searchinprogressmode": "#007ACC",
    "vim.statusBarColors.easymotionmode": "#007ACC",
    "vim.statusBarColors.easymotioninputmode": "#007ACC",
    "vim.statusBarColors.surroundinputmode": "#007ACC",
    "vsicons.dontShowNewVersionMessage": true,
    "window.zoomLevel": -1,
    "workbench.colorCustomizations": {
        "statusBar.background": "#957DAD",
        "statusBar.noFolderBackground": "#957DAD",
        "statusBar.debuggingBackground": "#957DAD"
    },
    "workbench.colorTheme": "One Dark Pro",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 7,
    "workbench.iconTheme": "vscode-icons",
}

